msgid ""
"<p>Dear {{ order.customer }},</p>\n"
"<p>A shipment has been created for your order and here are the details:</p>\n"
"\n"
"{% if shipment.tracking_code %}\n"
"<p>Tracking code: {{ shipment.tracking_code }}</p>\n"
"{% endif %}\n"
"\n"
"<p>Contents:</p>\n"
"<table>\n"
"    <thead>\n"
"        <tr>\n"
"            <th>Product</th>\n"
"            <th>Quantity</th>\n"
"        </tr>\n"
"    </thead>\n"
"    <tbody>\n"
"{% for shipment_product in shipments.total_products %}\n"
"        <tr>\n"
"            <td>{{ shipment_product.product }}</td>\n"
"            <td>{{ shipment_product.quantity }}</td>\n"
"        </tr>\n"
"{% endif %}\n"
"    </tbody>\n"
"</table>\n"
msgstr ""
"<p>Vážený {{ order.customer }},</p>\n"
"<p>Bola vytvorená zásielka pre vašu objednávku a tu sú podrobnosti:</p>\n"
"\n"
"{% if shipment.tracking_code %}\n"
"<p>Kód sledovania zásielky: {{ shipment.tracking_code }}</p>\n"
"{% endif %}\n"
"\n"
"<p>Obsah:</p>\n"
"<table>\n"
"    <thead>\n"
"        <tr>\n"
"            <th>Produkt</th>\n"
"            <th>Množstvo</th>\n"
"        </tr>\n"
"    </thead>\n"
"    <tbody>\n"
"{% for shipment_product in shipments.total_products %}\n"
"        <tr>\n"
"            <td>{{ shipment_product.product }}</td>\n"
"            <td>{{ shipment_product.quantity }}</td>\n"
"        </tr>\n"
"{% endif %}\n"
"    </tbody>\n"
"</table>\n"

msgid "Send Shipment Created Email"
msgstr "Poslať email o vytvorení zásielky"

msgid "Send an email to customer when a shipment is created."
msgstr "Po vytvorení zásielky poslať email zákazníkovi."

msgid ""
"This script will send an email to customer when a shipment of his order has "
